  "homepage": "https://github.com/jackpassero/window-a3x",
  "keywords": [
    "AutoItX3.Control"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Uladzimir Lauretsky",
      "email": "spamtut@tut.by"
    }
  ],
